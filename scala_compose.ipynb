   "source": [
    "def compose[A, B, C](\n",
    "    f: A => B,\n",
    "    g: B => C,\n",
    "): A => C = {\n",
    "    def op(value: A): C = g(f(value))\n",
    "    op\n",
    "}\n",
    "\n",
    "def addOne(v: Int): Int = v + 1\n",
    "def half(v: Int): Double = v/2.0\n",
    "\n",
    "val halfAddOne = compose(addOne, half)\n",
    "println(halfAddOne(5))"
   ]
  }
 ],
